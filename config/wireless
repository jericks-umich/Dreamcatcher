
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/ar934x_wmac'
	option htmode 'HT20'
	option country 'US'
	option txpower '12'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel '36'
	option hwmode '11a'
	option path 'pci0000:00/0000:00:00.0'
	option htmode 'HT20'
	option disabled '0'
	option txpower '8'
	option country 'US'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option ssid 'TEST NETWORK DO NOT USE'
	option encryption 'wpa2'
	option server '127.0.0.1'
	option key '674392384'
	option dynamic_vlan '2'
	option 'vlan_naming' '2'
	option 'vlan_bridge' 'br-lan'
